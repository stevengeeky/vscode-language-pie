{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Pie",
	"patterns": [
		{ "include": "#keywords" },
		{ "include": "#strings" }
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "variable.pie",
				"match": "\\b((?=\\\\()\\w+)\\b"
			}, {
				"name": "markup.underline",
				"match": "\\b(test)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.pie",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.pie",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.pie"
}